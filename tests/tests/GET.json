{
  "meta": {
    "description": "Get posts",
    "order": -1
  },
  "request": {
    "url": "http://jsonplaceholder.typicode.com/posts",
    "method": "GET",
    "headers": [
      "Accept: application/json"
    ]
  },
  "response": {
    "statusCode": 200,
    "headers": {
      "Content-Type": "application/json; charset=utf-8"
    },
    "ruleObjects": [{
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "unsignedinteger"
          },
          "id": {
            "type": "unsignedinteger"
          },
          "title": {
            "type": "string"
          },
          "body": {
            "type": "string"
          }
        },
        "required": ["userId", "id", "title", "body"]
      }
    }],
    "export": {
      "postId": "id"
    }
  }
}
