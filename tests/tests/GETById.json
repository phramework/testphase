{
  "meta": {
    "description": "Get post",
    "order": 0
  },
  "request": {
    "url": "http://jsonplaceholder.typicode.com/posts/{{postId}}",
    "method": "GET",
    "headers": []
  },
  "response": {
    "statusCode": [200, 201, 204],
    "ruleObjects": [{
      "type": "object",
      "properties": {
        "userId": {
          "type": "unsignedinteger",
          "enum": ["{{postId}}"]
        },
        "id": {
          "type": "unsignedinteger"
        },
        "title": {
          "type": "string"
        },
        "body": {
          "type": "string"
        }
      },
      "required": ["userId", "id", "title", "body"]
    }]
  }
}
