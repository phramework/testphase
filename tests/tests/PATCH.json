{
  "meta": {
    "description": "PATCH post",
    "order": 1
  },
  "request": {
    "url": "http://jsonplaceholder.typicode.com/posts/{{postId}}",
    "method": "PATCH",
    "headers": [
      "Accept: application/json",
      "Content-Type: application/json; charset=utf-8"
    ],
    "body": {
      "title": "foo"
    }
  },
  "response": {
    "statusCode": 200,
    "headers": {
      "Content-Type": "application/json; charset=utf-8"
    },
    "body": [{
      "type": "object",
      "properties": {
        "id": {
          "type": "unsignedinteger",
          "enum": ["{{{postId}}}"]
        }
      },
      "required": ["id"]
    }]
  }
}
